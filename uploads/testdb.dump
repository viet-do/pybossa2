-- TOC entry 243 (class 1259 OID 43909)
-- Name: users_rank; Type: MATERIALIZED VIEW; Schema: public; Owner: pybossa
--

CREATE MATERIALIZED VIEW public.users_rank AS
 WITH scores AS (
         SELECT "user".id,
            "user".created,
            "user".email_addr,
            "user".name,
            "user".fullname,
            "user".locale,
            "user".api_key,
            "user".passwd_hash,
            "user".ldap,
            "user".admin,
            "user".pro,
            "user".privacy_mode,
            "user".restrict,
            "user".category,
            "user".flags,
            "user".twitter_user_id,
            "user".facebook_user_id,
            "user".google_user_id,
            "user".ckan_api,
            "user".newsletter_prompted,
            "user".valid_email,
            "user".confirmation_email_sent,
            "user".subscribed,
            "user".consent,
            "user".info,
            "user".user_pref,
            "user".notified_at,
            "user".native_speaker,
            "user".city,
            "user".state,
            count(task_run.user_id) AS score
           FROM (public."user"
             LEFT JOIN public.task_run ON ((task_run.user_id = "user".id)))
          WHERE ("user".restrict = false)
          GROUP BY "user".id
        )
 SELECT scores.id,
    scores.created,
    scores.email_addr,
    scores.name,
    scores.fullname,
    scores.locale,
    scores.api_key,
    scores.passwd_hash,
    scores.ldap,
    scores.admin,
    scores.pro,
    scores.privacy_mode,
    scores.restrict,
    scores.category,
    scores.flags,
    scores.twitter_user_id,
    scores.facebook_user_id,
    scores.google_user_id,
    scores.ckan_api,
    scores.newsletter_prompted,
    scores.valid_email,
    scores.confirmation_email_sent,
    scores.subscribed,
    scores.consent,
    scores.info,
    scores.user_pref,
    scores.notified_at,
    scores.native_speaker,
    scores.city,
    scores.state,
    scores.score,
    row_number() OVER (ORDER BY scores.score DESC) AS rank
   FROM scores
  WITH NO DATA;


ALTER TABLE public.users_rank OWNER TO pybossa;

